{
  "format": "custom",
  "id": "deepdive",
  "label": "Deepdive",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "date": {
        "type": "Text",
        "config": {
          "label": "Date",
          "placeholder": ""
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Image",
          "constraint": {
            "width": 1920
          },
          "thumbnails": []
        }
      },
      "connected_person": {
        "type": "Link",
        "config": {
          "label": "Connected Person ",
          "select": "document",
          "repeat": false,
          "customtypes": [
            {
              "id": "person",
              "fields": ["title", "text"]
            }
          ]
        }
      },
      "text": {
        "type": "StructuredText",
        "config": {
          "label": "Text",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,hyperlink,list-item,o-list-item,strong,em"
        }
      },
      "key_takeaways": {
        "type": "StructuredText",
        "config": {
          "label": "Key Takeaways",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em,hyperlink,list-item,o-list-item"
        }
      }
    }
  }
}
